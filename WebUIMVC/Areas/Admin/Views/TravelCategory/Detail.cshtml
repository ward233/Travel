@model Model.TravelCategory

@{
    ViewBag.Title = "查看详情";
}

@section head
{
    <style>
        .txt-title,.txt-content {
            margin-bottom: 30px;
        }
    </style>
}

<div class="panel panel-headline">
    <div class="panel-heading">
        <h3 class="panel-title">查看旅游项目详情</h3>

    </div>
    <div class="panel-body text-center">
        <h1 class="txt-title">
            @Model.Title
        </h1>
            
        <div  class="txt-content">
            @Html.Raw(Model.Content)
        </div>
            
        <a href="@ViewBag.UrlRefer" runat="server" id="GoBack" class="btn btn-primary btn-lg">返回</a>

    </div>
</div>
